{
  "name": "dreamteam",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore client/\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/los-programadores/dreamteam.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/los-programadores/dreamteam/issues"
  },
  "homepage": "https://github.com/los-programadores/dreamteam#readme",
  "dependencies": {
    "animate.css": "^3.7.0",
    "axios": "^0.18.1",
    "body-parser": "^1.20.1",
    "boostrap": "^2.0.0",
    "concurrently": "^4.1.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.4.13",
    "nodemon": "^1.18.10",
    "react-bootstrap": "^1.0.0-beta.5"
  }
}
